def fac(n, k):
    if k == 0:
        return 1
    t = n
    i = n
    b = k
    l = n-k
    while i > l +1:
        n = n -1
        i = i -1
        t = t * n
    while k > 1:
        k = k -1
        b = b * k 
    print t
    print b
    return t//b

def partial(n, l, i):
    while i > l +1:
        i = i -1
        n = n * i
            
print fac(0, 0)